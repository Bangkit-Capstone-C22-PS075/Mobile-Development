<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_main.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_userFragment"
            app:destination="@id/userFragment" />
        <action
            android:id="@+id/action_profileFragment_to_createStroreFragment"
            app:destination="@id/createStroreFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />

    <fragment
        android:id="@+id/userFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/searcFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.SearcFragment"
        android:label="fragment_searc"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/createStroreFragment"
        android:name="com.jn.capstoneproject.d_jahit.ui.fragment.CreateSellerFragment"
        android:label="fragment_create_strore"
        tools:layout="@layout/fragment_create_seller" />
</navigation>